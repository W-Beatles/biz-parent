<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>cn.waynechu</groupId>
    <artifactId>app-parent</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>app-parent</name>
    <description>App parent for Spring Boot</description>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.1.0.RELEASE</version>
        <relativePath/>
    </parent>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
        <maven.test.skip>true</maven.test.skip>

        <app.version>0.0.1-SNAPSHOT</app.version>
        <tuhu-boot-starter-common.version>1.0.2018082802</tuhu-boot-starter-common.version>
        <tuhu-boot-starter-dynamic-datasource.version>1.0.2-SNAPSHOT</tuhu-boot-starter-dynamic-datasource.version>
        <rabbitmq-logback-appender.version>1.0.2018112101</rabbitmq-logback-appender.version>

        <poi.version>3.9</poi.version>
        <xmlbeans.version>2.3.0</xmlbeans.version>
        <fastjson.version>1.2.51</fastjson.version>
        <druid.version>1.1.6</druid.version>
        <pagehelper.spring.boot.version>1.2.9</pagehelper.spring.boot.version>
        <mybatis.spring.boot.version>1.3.2</mybatis.spring.boot.version>
        <mybatis-plus-boot-starter.version>3.0.6</mybatis-plus-boot-starter.version>
        <logstash.logback.encoder.version>4.7</logstash.logback.encoder.version>
        <springfox.swagger2.version>2.9.2</springfox.swagger2.version>
    </properties>

